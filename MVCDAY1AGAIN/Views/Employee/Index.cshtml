@using MVCDAY1AGAIN.DAL.Entites
@model List<Employee>

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">ID</th>
            <th scope="col">NAME</th>
            <th scope="col">AGE</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model )
{
        <tr>
            <th scope="row">3</th>
            <td>@data.ID</td>
            <td>@data.Name</td>
            <td>@data.Age</td>
        </tr>
}
    </tbody>
</table>

